{
  "compilerOptions": {
    "jsx": "react",
    "newLine": "lf",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["dom", "es5", "es6", "es7", "es2015.promise", "es2017.object"]
  }
}
